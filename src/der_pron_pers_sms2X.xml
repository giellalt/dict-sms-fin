   <e>
      <lg>
         <l pos="Pron" type="Pers">mon</l>
      </lg>
      <mg>
         <tg xml:lang="fin">
            <t pos="Pron" type="Pers">minä</t>
            <xg>
               <x>Mon leäm.</x>
               <xt>Minä olen.</xt>
            </xg>
            <xg>
               <x>Muʹst lie puäʒʒ.</x>
               <xt>Minulla on poro.</xt>
            </xg>
         </tg>
         <tg xml:lang="eng">
            <t pos="Pron" type="Pers">I</t>
            <xg>
               <x>Mon leäm</x>
               <xt>I am</xt>
            </xg>
            <xg>
               <x>Muʹst lie puäʒʒ.</x>
               <xt>I have a reindeer.</xt>
            </xg>
         </tg>
         <tg xml:lang="nob">
            <t pos="Pron" type="Pers">jeg</t>
            <xg>
               <x>Mon leäm</x>
               <xt>Jeg er</xt>
            </xg>
            <xg>
               <x>Muʹst lie puäʒʒ.</x>
               <xt>Jeg har en rein.</xt>
            </xg>
         </tg>
         <tg xml:lang="rus">
            <t pos="Pron" type="Pers">Я</t>
            <xg>
               <x>Muʹst lie puäʒʒ.</x>
               <xt>У меня есть северный олень.</xt>
            </xg>
         </tg>
      </mg>
   </e>
   <!--  since the analyzer returns:

         muäna   mon+Pron+Pers+Du+Nom
         muäna   muäna+Pron+Pers

         we need to add a paradigm here

        -->
